{
  "manifest_version": 2,
  "name": "Narwallets V3",
  "description": "NEAR protocol wallet",
  "version": "3.0.0",
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "page": "background/background.html",
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "alarms"
  ],
  "icons": {
    "48": "icons/logo_48.png",
    "128": "icons/logo_128.png"
  }
}